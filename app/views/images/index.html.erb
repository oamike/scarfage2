<p id="notice"><%= notice %></p>

<h1>Images</h1>

<table>
  <thead>
    <tr>
      <th>Item</th>
      <th>Integer</th>
      <th>User</th>
      <th>Integer</th>
      <th>Name</th>
      <th>String</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @images.each do |image| %>
      <tr>
        <td><%= image.item_id %></td>
        <td><%= image.integer %></td>
        <td><%= image.user_id %></td>
        <td><%= image.integer %></td>
        <td><%= image.name %></td>
        <td><%= image.string %></td>
        <td><%= image.status %></td>
        <td><%= link_to 'Show', image %></td>
        <td><%= link_to 'Edit', edit_image_path(image) %></td>
        <td><%= link_to 'Destroy', image, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Image', new_image_path %>
